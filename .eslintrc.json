{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "linebreak-style": "off",
        "max-len": "off",
        "space-before-blocks":"off",
        "func-call-spacing":"off",
        "quotes":"off",
        "no-unused-vars":"off"
    }
}
